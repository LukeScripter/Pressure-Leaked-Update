local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Vxplors Hub (Pressure)", "Sentinel")
local Tab = Window:NewTab("Main Menu")
local Section = Tab:NewSection("Infinite Yield")
Section:NewButton("Infinite Yield", "?", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    print("Clicked")
end)
local Section = Tab:NewSection("God Mode")
Section:NewButton("God Mode", "Made By LukeScripter", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/ltseverydayyou/uuuuuuu/main/pressure%20god.lua"))()
    print("Clicked")
end)
local Section = Tab:NewSection("Keyboard Mobile")
Section:NewButton("Keyboard", "?", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/GGH52lan/GGH52lan/main/keyboard.txt"))()
    print("Clicked")
end)
local Tab = Window:NewTab("Credits")
local Section = Tab:NewSection("Credits")
Section:NewLabel("Made By LukeScripter")
local Section = Tab:NewSection("Toggle Gui With Key Bind F")
Section:NewKeybind("F", "?", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
game.StarterGui:SetCore("SendNotification", { Title = "Pressure"; Text = "Made by LukeScripter. Subscribe him on youtube"; Icon = ""; Duration = 10; })
game.StarterGui:SetCore("SendNotification", { Title = "Notification"; Text = "Hope You Enjoy The Script!"; Icon = ""; Duration = 15; })
